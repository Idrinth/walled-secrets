{% include 'header.twig' %}
<h1>{{ title }}</h1>
<table>
    <thead>
        <tr>
            <th>Created</th>
            <th>User</th>
            <th>Resource</th>
            <th>Action</th>
            <th>IP</th>
        </tr>
    </thead>
    <tbody>
        {% for log in entries %}
            <tr>
                <th>{{ log.created }}</th>
                <td>{{ log.display }}</td>
                <td>{{ log.target }}@{{ log.type }}</td>
                <td>{{ log.action }}</td>
                <td>{{ log.ip }}</td>
            </tr>
        {% endfor %}
    </tbody>
</table>
{% include 'footer.twig' %}